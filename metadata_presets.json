{
  "mm2wood": {
    "title": [
      {
        "if_field": "Version",
        "if_operator": "is latest version",
        "if_value": "",
        "then_template": "{Title}"
      },
      {
        "if_field": "Version",
        "if_operator": "is not latest version",
        "if_value": "",
        "then_template": "{Title} (v{Version})"
      }
    ],
    "artist": [
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Neuro & Evil",
        "then_template": "{Artist} (feat. Evil & Neuro-sama)"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Neuro",
        "then_template": "{Artist} (feat. Neuro-sama)"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Evil",
        "then_template": "{Artist} (feat. Evil)"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "{Artist} (feat. {CoverArtist})"
      }
    ],
    "album": [
      {
        "if_field": "Discnumber",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "NEURO-SAMA KARAOKE COVERS: VOL {Discnumber}"
      }
    ]
  },
  "Default": {
    "title": [
      {
        "if_field": "Title",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "{Title}"
      }
    ],
    "artist": [
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Neuro",
        "then_template": "Neuro - {Artist}"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Evil",
        "then_template": "Evil - {Artist}"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "Duet ({CoverArtist}) - {Artist}"
      }
    ],
    "album": [
      {
        "if_field": "Discnumber",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "Disc {Discnumber}"
      }
    ]
  },
  "TuruuMGL": {
    "title": [
      {
        "if_field": "CoverArtist",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "{Title}"
      }
    ],
    "artist": [
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Neuro",
        "then_template": "({CoverArtist}.v{Version}) {Artist}"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Evil",
        "then_template": "({CoverArtist}.v{Version}) {Artist}"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is",
        "if_value": "Neuro & Evil",
        "then_template": "(Duet.v{Version}) {Artist}"
      },
      {
        "if_field": "CoverArtist",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "({CoverArtist}) {Artist}"
      }
    ],
    "album": [
      {
        "if_field": "Discnumber",
        "if_operator": "is not empty",
        "if_value": "",
        "then_template": "Disc {Discnumber}"
      }
    ]
  }
}